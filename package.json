{
    "name": "geo-vis",
    "type": "module",
    "version": "0.8.0",
    "private": true,
    "description": "web app for geometric visuals",
    "keywords": [
        "astro",
        "typescript",
        "tailwindcss",
        "fractals",
        "geometry",
        "svg",
        "alpinejs"
    ],
    "license": "MIT",
    "author": "xenitane",
    "homepage": "https://xenitane.xyz/geo-vis",
    "engines": {
        "node": ">=18"
    },
    "browserList": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "scripts": {
        "dev": "astro dev",
        "build": "astro check && astro build",
        "preview": "astro preview",
        "astro": "astro",
        "format": "prettier --write \"**/*.{js,ts,css,json,html,md,mdx,yml,yaml,astro,mdx,jsx,tsx,svelte,vue}\"",
        "reset": "rm -rf ./dist"
    },
    "dependencies": {
        "@fontsource/geist-sans": "^5.2.5",
        "alpinejs": "^3.14.9",
        "tailwindcss": "^4.1.11"
    },
    "devDependencies": {
        "@astrojs/alpinejs": "^0.4.8",
        "@astrojs/check": "^0.9.4",
        "@tailwindcss/vite": "^4.1.12",
        "@types/alpinejs": "^3.13.11",
        "@vite-pwa/astro": "^1.1.0",
        "astro": "^5.12.8",
        "clsx": "^2.1.1",
        "prettier": "^3.6.2",
        "prettier-plugin-astro": "^0.14.1",
        "prettier-plugin-organize-imports": "^4.2.0",
        "prettier-plugin-tailwindcss": "^0.6.14",
        "tailwind-merge": "^3.3.1",
        "typescript": "^5.9.2",
        "vite-plugin-pwa": "^1.0.2",
        "workbox-window": "^7.3.0"
    }
}
